    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block head_title %}{{ site.title }} | {{ site.tagline }}{% endblock %}</title>

    <link href="{{ site_root }}assets/theme/css/style.css" rel="stylesheet">


