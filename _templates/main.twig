<body>

<header id="site-header">
    <div class="container">
    <h1>
        <a href="{{ site_root }}">{{ site.title }}</a>
    </h1>
        </div>
</header>


<div class="container">




    <div class="row">
        <div class="col-sm-3 sidebar">


                {% include 'side.twig' %}

        </div>
        <div class="col-sm-9">
            {% block body %}
                <p>Missing body.</p>
            {% endblock %}
        </div>
    </div>
</div>

<footer id="site-footer">
    <div class="container">
        <hr>
        <p class="text-muted">Powered by Nog, a NowZoo thing | Nog and this content are licensed under MIT</p>
    </div>

</footer>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>

{% block footer_scripts %}{% endblock %}
</body>
